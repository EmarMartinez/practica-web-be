{
  "name": "practica-web-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node main.js",
    "dev": "nodemon main.js --ignore config/locales/ --ignore data/",
    "debug:win": "set DEBUG=express:* & npm run dev",
    "debug:linux": "DEBUG=express:* npm run dev",
    "create-resource": "node create-resource",
    "excel2json": "node excel2json",
    "lint": "eslint ./**/*.js",
    "format:test": "prettier **/*.{js,html,md,css,json} --list-different",
    "format:write": "prettier **/*.{js,html,md,css,json} --write",
    "pull": "git pull && git pull origin develop --no-ff"
  },
  "_moduleAliases": {
    "@root": ".",
    "@config": "config",
    "@data": "data",
    "@loaders": "loaders",
    "@factories": "factories",
    "@middlewares": "middlewares",
    "@api": "routes/api",
    "@shared": "shared"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "console-stamp": "^3.0.0-rc4.3",
    "convert-excel-to-json": "^1.7.0",
    "cookie-parser": "~1.4.4",
    "date-fns": "^2.22.1",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-fileupload": "^1.4.0",
    "http-errors": "~1.6.3",
    "i18n": "^0.13.3",
    "inflection": "^1.13.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "yargs": "^17.3.1"
  }
}